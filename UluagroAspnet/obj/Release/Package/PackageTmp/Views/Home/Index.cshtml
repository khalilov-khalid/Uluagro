@model UluagroAspnet.Mainclasses.MainViewModel
@{
    ViewBag.Title = "Home Page";
}


<header class="header clear w-100p">
    <a href="/Home/Index" title="UluAgro" class="logo float-left" lang="en">
        <img src="~/Public/assets/images/logo.png" alt="UluAgro Logo" height="50" width="">
    </a>
    <div id="temperature">
        <span class="morning_temp"></span>
        <span class="weather_ico"></span>
    </div>
</header>

<!-- Right Side-->
<aside class="right-side clear d-table text-center">
    <div class="d-row">
        <div class="d-cell">
            @*<a href="#" class="account-toggle d-block" data-toggle="tooltip" data-placement="left" title="@Model._loginedUser.Worker.Name  @Model._loginedUser.Worker.Surname">
                    <img src="~/Public/assets/images/user-avatar.png" alt="@Model._loginedUser.Worker.Name  @Model._loginedUser.Worker.Surname" width="45" height="45" class="radius-50p">
                </a>*@
            <a href="#" class="account-toggle side-toggle d-block relative" data-toggle="tooltip" data-placement="left" title="@Model._loginedUser.Worker.Name  @Model._loginedUser.Worker.Surname" aria-label="@Model._loginedUser.Worker.Name  @Model._loginedUser.Worker.Surname" id="account-menu" aria-haspopup="true" aria-expanded="false">

                <span id="NotficationCount" class="notify-count badge animation-2s zoomIn infinite bg-orange radius-10" hidden="hidden">2</span>

                <img src="~/Public/assets/images/user-avatar.png" alt="@Model._loginedUser.Worker.Name  @Model._loginedUser.Worker.Surname" width="45" height="45" class="radius-50p">
            </a>
        </div>
    </div>

    <div class="d-row h-100p">
        <div class="d-cell menu-toggles">
            <span class="side-toggle btn no-border" role="button"
                  data-toggle="tooltip" data-placement="left"
                  title="Bitkilər və Planlamalar" aria-label="Bitkilər və Planlamalar"
                  id="side-menu-1" aria-haspopup="true" aria-expanded="false">
                <i aria-hidden="true" class="icon-leaf v-align-middle"></i>
            </span>

            <span class="side-toggle btn no-border" role="button"
                  data-toggle="tooltip" data-placement="left"
                  title="Obyektlər" aria-label="Obyektlər"
                  id="side-menu-2" aria-haspopup="true" aria-expanded="false">
                <i aria-hidden="true" class="icon-th-circle v-align-middle"></i>
            </span>

            <span class="side-toggle btn no-border" role="button"
                  data-toggle="tooltip" data-placement="left"
                  title="Statistikalar" aria-label="Statistikalar"
                  id="side-menu-3" aria-haspopup="true" aria-expanded="false">
                <i aria-hidden="true" class="icon-survey-list v-align-middle"></i>
            </span>

            <span class="side-toggle btn no-border" role="button"
                  data-toggle="tooltip" data-placement="left"
                  title="Kargüzarlıq" aria-label="Kargüzarlıq"
                  id="side-menu-4" aria-haspopup="true" aria-expanded="false">
                <i aria-hidden="true" class="icon-book v-align-middle"></i>
                @if (Model.docmsgcount != 0)
                {
                    <span class="document_message_not"></span>
                }
            </span>

            <span class="side-toggle btn no-border" role="button"
                  aria-hidden="true" aria-label="Xəritə tarixçəsi"
                  id="map-type" data-toggle="tooltip" data-placement="left"
                  title="Xəritə tarixçəsi" aria-haspopup="true" aria-expanded="false">
                <i aria-hidden="true" class="icon-history v-align-middle"></i>
            </span>
        </div>
    </div>

    <div class="d-row">
        <div class="d-cell">
            <a href="http://agrotv.az/" rel="external noopener noreferrer" class="d-block" data-toggle="tooltip" data-placement="left" title="AgroTv">
                <i aria-hidden="true" class="icon-tv v-align-middle"></i>
            </a>
            <a href="http://agrobirja.az/" rel="external noopener noreferrer" class="d-block" data-toggle="tooltip" data-placement="left" title="Birja">
                <i aria-hidden="true" class="icon-currency-usd v-align-middle"></i>
            </a>
        </div>
    </div>

    <div class="d-row">
        <div class="d-cell">
            <span class="side-toggle btn no-border" role="button" aria-hidden="true" aria-label="Haqqımızda" id="about-us" data-toggle="tooltip" data-placement="left" title="Haqqımızda" aria-haspopup="true" aria-expanded="false">
                <i aria-hidden="true" class="icon-info-circle-thin v-align-middle"></i>
            </span>
            <a href="@Url.Action("Logout","LoginUser")" class="d-block" data-toggle="tooltip" data-placement="left" title="Çıxış">
                <i aria-hidden="true" class="icon-power-1 v-align-middle"></i>
            </a>
        </div>
    </div>

</aside>

<!-- Account -->
<aside class="sidebar lg tr-3s" aria-labelledby="account-menu">
    <div class="sidebar-head bg-special clear">
        <h2 class="m-0 text-white bold float-left">Bildirişlər</h2>
        <h2 class="m-0 text-white bold float-right">@Model._loginedUser.Worker.Name  @Model._loginedUser.Worker.Surname</h2>
    </div>

    <div class="sidebar-body p-20 text-white light font-16 scroll o-auto">

        <div class="clear">
            <div class="table-responsive no-border mb-40">
                <table class="table bg-tr-2">
                    <thead>
                        <tr>
                            <th>№</th>
                            <th class="no-wrap">Göndərən</th>
                            <th class="no-wrap">Göndərilmə tarixi</th>
                            <th>Bax</th>
                        </tr>
                    </thead>
                    <tbody id="NotficationMessages">
                        @*Bu da tamamlanmış bildiriş*@

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</aside>

